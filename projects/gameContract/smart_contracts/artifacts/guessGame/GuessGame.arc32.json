                    },
                    {
                        "type": "pay",
                        "name": "payment"
                    }
                ],
                "readonly": false,
                "returns": {
                    "type": "void"
                },
                "desc": "Allows a player to join the session."
            },
            {
                "name": "revealMove",
                "args": [
                    {
                        "type": "uint64",
                        "name": "sessionID",
                        "desc": "- The session ID."
                    },
                    {
                        "type": "uint64",
                        "name": "choice",
                        "desc": "- The player's chosen number (must be between 0 and 100)."
                    },
                    {
                        "type": "byte[]",
                        "name": "salt",
                        "desc": "- The secret salt used during the commit phase."
                    }
                ],
                "readonly": false,
                "returns": {
                    "type": "void"
                },
                "desc": "Reveals a player's move and updates global game statistics."
            },
            {
                "name": "claimWinnings",
                "args": [
                    {
                        "type": "uint64",
                        "name": "sessionID",
                        "desc": "- The session ID."
                    }
                ],
                "readonly": false,
                "returns": {
                    "type": "uint64",
                    "desc": "The amount won by the caller."
                },
                "desc": "Calculates the winner and distributes the prize using the Pull pattern.\nUses `ensureBudget` to guarantee sufficient opcodes for the calculation loop."
