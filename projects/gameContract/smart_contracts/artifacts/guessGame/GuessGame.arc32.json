            {
                "name": "createSession",
                "args": [
                    {
                        "type": "(uint64,uint64,uint64,uint64)",
                        "name": "config",
                        "desc": "- The game configuration (timelines, fees)."
                    },
                    {
                        "type": "pay",
                        "name": "mbrPayment",
                        "desc": "- Payment transaction to cover the Minimum Balance Requirement (MBR)."
                    }
                ],
                "readonly": false,
                "returns": {
                    "type": "uint64",
                    "desc": "The unique session ID."
                },
                "desc": "Initializes a new game session and allocates storage for counters."
            },
            {
                "name": "joinSession",
                "args": [
                    {
                        "type": "uint64",
                        "name": "sessionID"
                    },
                    {
                        "type": "byte[]",
